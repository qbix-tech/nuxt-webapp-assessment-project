{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watch=true",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:deploy": "drizzle-kit push --verbose --strict"
  },
  "lint-staged": {
    "**/*.{js,jsx,cjs,mjs,json,ts,tsx,html,css,json,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts}": "vitest related --run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@libsql/client": "^0.14.0",
    "@nuxt/content": "^2.13.2",
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/fonts": "^0.9.2",
    "@nuxt/image": "^1.8.0",
    "@nuxt/ui": "^2.18.6",
    "@paralleldrive/cuid2": "^2.2.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue-email/components": "^0.0.19",
    "@vue-email/render": "^0.0.9",
    "@vuepic/vue-datepicker": "^9.0.3",
    "@vueuse/components": "^11.2.0",
    "@vueuse/integrations": "^11.1.0",
    "@vueuse/router": "^11.1.0",
    "drizzle-orm": "^0.34.0",
    "emailjs": "^4.0.3",
    "jsdom": "^25.0.1",
    "nanoid": "^5.0.7",
    "nuxt": "^3.13.0",
    "nuxt-auth-utils": "^0.4.2",
    "qrcode": "^1.5.4",
    "validator": "^13.12.0",
    "vue": "latest",
    "vue-router": "^4.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify-json/simple-icons": "^1.2.10",
    "@nuxt/test-utils": "^3.14.2",
    "@playwright/test": "^1.47.2",
    "@types/jsdom": "^21.1.7",
    "@types/validator": "^13.12.2",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.1.0",
    "@vueuse/nuxt": "^11.1.0",
    "drizzle-kit": "^0.25.0",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright-core": "^1.47.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  }
}
